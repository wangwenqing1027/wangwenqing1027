<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.datou.demo.dao.UserInfoMapper">

    <select id="getUserInfo" resultType="com.datou.demo.model.domain.UserInfoDO">
      SELECT * from datou_user_info where name=#{name} and pwd=#{pwd}
    </select>
</mapper>